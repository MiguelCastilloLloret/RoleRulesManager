{# app/Resources/views/lucky/rolManage.html.twig #}

{% extends 'base.html.twig' %}

{% block stylesheets %}
       <link href="{{ asset('css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
       <link href="{{ asset('css/main.css') }}" type="text/css" rel="stylesheet" />
{% endblock %}

{% block body %}
	{% include './index/header.html.twig' %}
	<div class="container contenedor">
	    <div class="block bloque">
	    	<h1>{{titulo}}</h1>
	    	<div class="wrap">
		    	<a class="btn btn-primary" href="./{{ juego }}/crear">Crear Personaje</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/modificar">Modificar Personaje</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/eliminar">Eliminar Personaje</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/crearPlantilla">Crear Plantilla</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/eliminarPlantilla">Eliminar Plantilla</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/crearArma">Crear Arma</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/modificarArma">Modificar Arma</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/crearPartida">Crear Partida</a>
		    	<a class="btn btn-primary" href="./{{ juego }}/eliminarPartida">Eliminar Partida</a>
		    </div>
		</div>
		<div class="block">
			{{ hola }}
		</div>
	</div>
	<!-- jQuery (necessary for Bootstraps JavaScript plugins) -->
    <script src="{{ asset('js/jquery.js') }}"></script>
    <!-- Include all JavaScripts, compiled by Assetic -->
    <script src="{{ asset('js/bootstrap.js') }}"></script>
{% endblock %}